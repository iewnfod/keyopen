{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "../src",
    "distDir": "../src",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "keyopen",
    "version": "1.0.5"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "dialog": {
        "all": true
      },
      "globalShortcut": {
        "all": true
      },
      "window": {
        "all": true
      }
    },
    "systemTray": {
      "iconPath": "icons/icon_black.png",
      "iconAsTemplate": true
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.iewnfod.keyopen",
      "icon": [
        "icons/icon.icns"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "",
        "fullscreen": false,
        "resizable": true,
        "width": 1000,
        "height": 700,
        "label": "main",
        "focus": true,
        "minWidth": 650,
        "minHeight": 400,
        "visible": false
      }
    ]
  }
}
